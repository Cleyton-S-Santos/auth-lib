{
  "name": "@cleytonsousa/auth-lib",
  "version": "0.1.5",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cleyton-S-Santos/auth-lib.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "description": "Biblioteca de autenticação Node/TypeScript com clean architecture",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build && npm run typecheck && npm run lint"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "vitest": "^2.1.3",
    "@types/node": "^22.7.5",
    "@eslint/js": "^9.13.0",
    "eslint": "^9.13.0",
    "@typescript-eslint/parser": "^8.12.2",
    "@typescript-eslint/eslint-plugin": "^8.12.2"
  }
}
